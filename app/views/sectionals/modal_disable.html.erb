<%= render 'shared/modal', title: "Dar de baja seccional", fade_in: true do %>
  <p>Se va a dar de baja la seccional <b><%= @sectional.name %></b></p>
  <%= form_with(url: disable_sectional_path(@sectional), method: 'PUT') do |form| %>
      <%= form.submit 'Dar de baja', class: 'btn btn-danger' %>
      <button type="button" class="btn btn-warning" data-bs-dismiss="modal">Cancelar</button>
    </div>
  <% end %>
<% end %>