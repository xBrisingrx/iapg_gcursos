<%= tag.tr id: dom_id(headquarter) do %>
  <td><%= headquarter.name %></td>
  <td><%= headquarter.description %></td>
  <td>
    <%= link_to edit_headquarter_path(headquarter), 
      class: "btn btn-warning btn-sm btn-sm btn-icon text-white",title: 'Editar' ,data: { turbo_frame: "modal"} do %>
      <i class="bi bi-pencil-square"></i>
    <% end %>
    <%= link_to modal_disable_headquarter_path(headquarter), 
      class: "btn btn-sm btn-icon btn-danger",title: 'Eliminar' ,data: { turbo_frame: "modal"} do %>
      <i class="bi bi-trash"></i>
    <% end %>
  </td>
<% end %>